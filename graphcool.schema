type File {
  contentType: String!
  createdAt: DateTime!
  id: ID!
  name: String!
  secret: String!
  size: Int!
  updatedAt: DateTime!
  url: String!
}

type Post {
  content: String
  createdAt: DateTime!
  description: String
  id: ID!
  imageUrl: String
  title: String
  updatedAt: DateTime!
}

type User {
  createdAt: DateTime!
  id: ID!
  updatedAt: DateTime!
}
